<% include header %>
<% if(entries.length) { %>
  <% entries.forEach(function(entry) { %>
    <div class="panel panel-default">
      <div class="panel-heading">
        <div class="text-muted pull-right">
          <%= entry.published %>
        </div>
        <%= entry.title %>
      </div>
      <div class="panel-body">
        <%= entry.body %>
      </div>
    </div>



  <%}) %>

<%}else {%>
  Записей нет: <a href="/new-entry">Добавить</a>
  
<%}%>
<% include footer%>
